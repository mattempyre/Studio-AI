# Backend server port
PORT=3001

# Frontend URL for CORS
FRONTEND_URL=http://localhost:3000

# Database path (relative to project root)
DATABASE_PATH=./data/studio.db

# Output directory for generated media (images, videos, audio)
# Files are organized as: {OUTPUT_DIR}/{projectId}/{type}/
OUTPUT_DIR=./data/projects

# Inngest configuration
# Dev server URL (runs via Docker)
INNGEST_DEV_SERVER=http://localhost:8288
# Event key for production (not needed for local dev)
# INNGEST_EVENT_KEY=your_event_key_here
# Signing key for production (not needed for local dev)
# INNGEST_SIGNING_KEY=your_signing_key_here

# External service API keys
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# ComfyUI server URL
COMFYUI_URL=http://localhost:8188

# Chatterbox TTS server URL
CHATTERBOX_URL=http://localhost:8004

# Node environment
NODE_ENV=development
